{
  "name": "@fecrash/workspace",
  "version": "1.0.0",
  "description": "TypeScript Crash",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start:api": "npm run build -w ./packages/api && npm run start:prod -w ./packages/api",
    "start:parang": "npm run start -w ./packages/parang",
    "start:console": "npm run start -w ./packages/console",
    "start:joy": "npm run start -w ./packages/joy",
    "start:namu": "npm run start -w ./packages/namu",
    "start:seal": "npm run start -w ./packages/seal",
    "start:yechu": "npm run start -w ./packages/yechu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/InSeong-So/TypeScriptCrash.git"
  },
  "author": "InSeong-So",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/InSeong-So/TypeScriptCrash/issues"
  },
  "homepage": "https://github.com/InSeong-So/TypeScriptCrash#readme",
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "^2.5.1",
    "prettier-eslint": "^13.0.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@date-io/moment": "^2.13.1",
    "chart.js": "^2.8.0",
    "@types/chart.js": "^2.7.0"
  }
}
